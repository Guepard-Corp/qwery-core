    {
      "id": "s3",
      "name": "S3 Object Storage",
      "description": "Connect to S3-compatible storage (AWS, DigitalOcean, MinIO) and query Parquet or JSON files with wildcard filtering",
      "icon": "/extensions/s3/s3.svg",
      "schema": {
        "type": "object",
        "properties": {
          "provider": {
            "type": "string",
            "enum": ["aws", "digitalocean", "minio", "other"],
            "description": "S3-compatible provider (endpoint URL required for non-AWS)"
          },
          "aws_access_key_id": {
            "type": "string",
            "description": "Access key ID"
          },
          "aws_secret_access_key": {
            "type": "string",
            "description": "Secret access key",
            "format": "password"
          },
          "aws_session_token": {
            "type": "string",
            "description": "Session token (optional, for temporary credentials)"
          },
          "region": {
            "type": "string",
            "description": "Region (e.g. us-east-1, or DigitalOcean Spaces region: fra1, nyc3)"
          },
          "endpoint_url": {
            "type": "string",
            "description": "Custom endpoint (required for MinIO/other). For DigitalOcean optional: omit to use https://<region>.digitaloceanspaces.com",
            "format": "uri"
          },
          "bucket": {
            "type": "string",
            "description": "Bucket name"
          },
          "prefix": {
            "type": "string",
            "description": "Prefix/folder inside bucket",
            "default": ""
          },
          "format": {
            "type": "string",
            "enum": ["parquet", "json"],
            "description": "File format to read (Parquet or JSON)"
          },
          "includes": {
            "type": "array",
            "description": "Wildcard include patterns (e.g. **/*.parquet). First used.",
            "items": {
              "type": "string"
            }
          },
          "excludes": {
            "type": "array",
            "description": "Wildcard exclude patterns",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "provider",
          "aws_access_key_id",
          "aws_secret_access_key",
          "region",
          "bucket",
          "format"
        ]
      },
      "packageName": "@qwery/extension-s3",
      "drivers": [
        {
          "id": "s3.duckdb",
          "name": "DuckDB (Node)",
          "description": "Node driver for S3 Parquet/JSON with wildcard support",
          "runtime": "node"
        }
      ],
      "formConfig": null
    },
