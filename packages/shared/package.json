{
  "name": "@qwery/shared",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "format": "prettier --check \"**/*.{ts,tsx}\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "validate:error-keys": "vitest run __tests__/validate-error-i18n.test.ts"
  },
  "prettier": "@qwery/prettier-config",
  "exports": {
    "./logger": "./src/logger/index.ts",
    "./model-cost": "./src/model-cost/index.ts",
    "./utils": "./src/utils.ts",
    "./hooks": "./src/hooks/index.ts",
    "./events": "./src/events/index.tsx",
    "./registry": "./src/registry/index.ts",
    "./desktop": "./src/desktop/index.ts",
    "./message-constants": "./src/message-constants.ts",
    "./message-role-utils": "./src/utils/message-role.ts",
    "./error": "./src/error/index.ts",
    "./export": "./src/export.ts"
  },
  "devDependencies": {
    "@qwery/eslint-config": "workspace:*",
    "@qwery/prettier-config": "workspace:*",
    "@qwery/tailwind-config": "workspace:*",
    "@qwery/tsconfig": "workspace:*",
    "@types/react": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "eslint": "^9.39.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@qwery/domain": "workspace:*",
    "papaparse": "^5.5.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "sql-formatter": "^15.6.11"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  }
}
