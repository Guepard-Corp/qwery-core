{
  "extends": "@qwery/tsconfig/base.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {}
  },
  "include": [
    "*.ts",
    "*.tsx",
    "src",
    "__evals__"
  ],
  "exclude": [
    "node_modules"
  ]
}