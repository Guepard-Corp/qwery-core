{
  "name": "@qwery/extension-gsheet-csv",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/driver.js",
  "types": "./dist/driver.d.ts",
  "exports": {
    ".": {
      "types": "./dist/driver.d.ts",
      "default": "./dist/driver.js"
    },
    "./schema": "./dist/schema.js"
  },
  "scripts": {
    "lint": "echo \"no lint configured\"",
    "test": "vitest run --coverage --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "contributes": {
    "datasources": [
      {
        "id": "gsheet-csv",
        "name": "Google Sheets (CSV)",
        "description": "Connect to public Google Sheets via CSV export",
        "docsUrl": "https://support.google.com/docs/answer/2494822",
        "supportsPreview": true,
        "tags": [
          "Files",
          "SaaS"
        ],
        "icon": "media/gsheet.svg",
        "drivers": [
          "gsheet-csv.duckdb"
        ]
      }
    ],
    "drivers": [
      {
        "id": "gsheet-csv.duckdb",
        "name": "DuckDB (Node)",
        "description": "Node runtime DuckDB driver for Google Sheets CSV",
        "runtime": "node",
        "entry": "./dist/driver.js"
      }
    ]
  },
  "dependencies": {
    "@duckdb/node-api": "1.4.2-r.1",
    "@qwery/extensions-sdk": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  }
}