{
  "name": "@qwery/extension-s3",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/driver.js",
  "types": "./dist/driver.d.ts",
  "exports": {
    ".": {
      "types": "./dist/driver.d.ts",
      "default": "./dist/driver.js"
    }
  },
  "scripts": {
    "lint": "echo \"no lint configured\"",
    "test": "vitest run --coverage --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "contributes": {
    "datasources": [
      {
        "id": "s3",
        "name": "S3 Object Storage",
        "description": "Connect to S3-compatible object storage (AWS, DigitalOcean, MinIO, etc) and query Parquet files",
        "icon": "media/s3.svg",
        "schema": {
          "type": "object",
          "properties": {
            "aws_access_key_id": {
              "type": "string",
              "description": "AWS access key ID"
            },
            "aws_secret_access_key": {
              "type": "string",
              "description": "AWS secret access key secret:true",
              "format": "password"
            },
            "region": {
              "type": "string",
              "description": "AWS region (e.g. us-east-1)"
            },
            "endpoint_url": {
              "type": "string",
              "description": "Custom S3 endpoint URL for compatible providers (DigitalOcean, MinIO, etc)",
              "format": "uri"
            },
            "bucket": {
              "type": "string",
              "description": "S3 bucket name"
            },
            "prefix": {
              "type": "string",
              "description": "Prefix (folder) inside the bucket to scan for data files",
              "default": ""
            },
            "includes": {
              "type": "array",
              "description": "Optional include patterns relative to the bucket (e.g. **/*.parquet). First match is used.",
              "items": {
                "type": "string"
              }
            },
            "excludes": {
              "type": "array",
              "description": "Optional exclude patterns (currently informational only)",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "aws_access_key_id",
            "aws_secret_access_key",
            "region",
            "bucket"
          ]
        },
        "drivers": [
          "s3.duckdb"
        ]
      }
    ],
    "drivers": [
      {
        "id": "s3.duckdb",
        "name": "DuckDB (Node)",
        "description": "Node runtime DuckDB driver for Parquet files in S3-compatible storage",
        "runtime": "node",
        "entry": "./dist/driver.js"
      }
    ]
  },
  "dependencies": {
    "@duckdb/node-api": "1.4.2-r.1",
    "@qwery/extensions-sdk": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  }
}